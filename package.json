{
  "name": "ng2-page-slider",
  "version": "0.8.0",
  "description": "Mimicks the functionality of UIPageViewController in pure HTML for mobile web apps with Angular 2",
  "repository": "https://github.com/KeatonTech/Angular-2-Page-Slider",
  "main": "index.ts",
  "scripts": {
    "build": "tsc -d",
    "watch": "tsc -d -w",
    "examples": "concurrently \"npm run watch\" \"npm run watchExamples\" \"npm run serveExamples\"",
    "serveExamples": "live-server --entry-file=./examples/index.html",
    "buildExamples": "tsc -p ./examples",
    "watchExamples": "tsc -w -p ./examples"
  },
  "keywords": [
    "Angular2",
    "NG2",
    "Typescript",
    "UIPageView",
    "UIPageViewController",
    "Pagination",
    "Slider",
    "Carousel"
  ],
  "author": "Keaton Brandt",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.3.0",
    "@angular/compiler": "^2.3.0",
    "@angular/core": "^2.3.0",
    "@angular/platform-browser": "^2.3.0",
    "@angular/platform-browser-dynamic": "^2.3.0",
    "systemjs": "0.19.41"
  },
  "devDependencies": {
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-rc.4",
    "zone.js": "^0.7.2",
    "concurrently": "^2.0.0",
    "live-server": "1.0.0",
    "typescript": "^2.0.10"
  }
}